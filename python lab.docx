@charset "utf-8";

html, body { margin: 0px; border: 0px; padding: 0px; background-color: white; direction: ltr; text-size-adjust: none; }

ul { list-style-type: none; padding: 0px; margin: 0px; border: 0px; }

table { font-size: inherit; border: 1px solid black; border-collapse: collapse; }

table td, th { padding: 0px 10px; }

.FootnoteEndnote { color: rgb(0, 0, 0); opacity: 0.8; padding: 0px 5% 0px 10%; }

.FootnoteEndnoteHr { background-color: rgb(0, 0, 0); border: none; color: rgb(0, 0, 0); height: 1.35px; opacity: 0.8; width: 90%; }

.icon { float: right; position: static; width: 40px; height: 40px; }

.iconN { margin: 0px 10px 0px 0px; }

.layer { position: absolute; width: 100%; height: 100%; }

.liveregion { left: 0px; top: -10px; width: 0px; height: 0px; overflow: hidden; z-index: -1; }

.loading { position: relative; top: 50%; left: 50%; margin: -12px 0px 0px -12px; }

.menu { position: absolute; top: 50%; left: 50%; border: 3px solid rgb(25, 71, 138); box-shadow: rgba(0, 0, 0, 0.2) 0px 0px 5px; background-color: rgb(255, 255, 255); }

.pageHrV2 { background-color: rgb(0, 0, 0); border: none; color: rgb(0, 0, 0); height: 2.7px; opacity: 0.45; }

.pageNumV2 { color: rgb(0, 0, 0); font-size: medium; margin: 10px 0px; opacity: 0.58; text-align: right; }

.pageNum { color: rgb(0, 0, 0); font-size: medium; margin: 10px 0px; opacity: 0.2; text-align: right; }

div .portrait { width: 40px; height: 240px; margin: -123px 0px 0px -23px; }

div .landscape { width: 240px; height: 40px; margin: -23px 0px 0px -123px; }

div .portrait .iconN { margin: 10px 0px 0px; }

@font-face { font-family: "Segoe UI Web"; }

#m_msgDialogLayerRevamp { display: table; text-align: center; background-color: rgba(211, 211, 211, 0.8); }

#m_msgDialogLayerRevamp .dialogVAlign { display: table-cell; position: relative; vertical-align: middle; }

#m_msgDialogRevamp { background-color: white; box-shadow: rgba(0, 0, 0, 0.2) 0px 0px 5px; display: inline-block; position: relative; width: 338px; }

#m_msgDialogRevamp .dialogBtn { border: 1px solid rgb(54, 119, 190); font-family: "Segoe UI", "Segoe UI Web", "Segoe UI semibold", sans-serif, -apple-system; font-size: 14px; height: 40px; line-height: 40px; margin: auto auto 10px; overflow: hidden; width: 244px; font-weight: 600; }

#m_msgTextAreaRevamp, #m_msgSessionIdAreaRevamp { font-family: "Segoe UI", "Segoe UI Web", "Segoe UI semibold", sans-serif, -apple-system; margin: 12px 46px 20px; max-height: 150px; overflow: hidden; line-height: 20px; text-align: center; font-weight: 400; }

#m_msgTextAreaRevamp { font-size: 14px; }

#m_msgSessionIdAreaRevamp { font-size: 9pt; }

#m_msgTitleAreaRevamp { font-family: "Segoe UI", "Segoe UI Web", "Segoe UI semibold", sans-serif, -apple-system; margin-top: 24px; margin-left: 40px; font-size: 20px; font-weight: 600; text-align: center; }

#m_msgDialogAreaRevamp { margin: 30px 0px; }

#m_msgDialogCloseButtonRevamp { float: right; margin: 8px 13px 5px 5px; zoom: 1.25; }

#m_msgDialogRevamp .installBtn { background: rgb(24, 90, 189); color: white; }

#m_msgDialogRevamp .continueBtn { margin-bottom: 25px; color: rgb(24, 90, 189); }

#m_msgDialog { background-color: white; border: 2px solid rgb(25, 71, 138); box-shadow: rgba(0, 0, 0, 0.2) 0px 0px 5px; display: inline-block; position: relative; width: 300px; }

#m_msgDialog .dialogBtn { border: 1px solid rgb(54, 119, 190); color: rgb(54, 119, 190); font-family: Arial; font-size: 10pt; display: inline-block; height: 22px; line-height: 22px; margin: 0px 5px 12px; overflow: hidden; width: 120px; }

#m_msgDialogLayer { display: table; text-align: center; }

#m_msgDialogLayer .dialogVAlign { display: table-cell; position: relative; vertical-align: middle; }

#m_msgTextArea, #m_msgSessionIdArea { color: rgb(153, 153, 153); font-family: Arial; margin: 12px 12px 24px; max-height: 150px; overflow: hidden; padding: 0px; text-align: left; }

#m_msgTextArea { font-size: 12pt; }

#m_msgSessionIdArea { font-size: 9pt; }

#m_originalPosition { position: absolute; top: 0px; right: 0px; width: 5px; height: 45px; background-color: rgb(102, 102, 102); }

#m_pageInfo { display: inline-block; position: relative; padding: 0px 10px; text-align: center; height: 45px; line-height: 45px; background-color: rgba(226, 226, 226, 0.89); color: black; }

#m_toolbarMenu { width: 140px; height: 40px; margin: -23px 0px 0px -73px; }

#m_indicator { width: 25px; height: 45px; position: absolute; top: 50%; left: 0px; margin: -23px 0px 0px; }

#m_content { left: 20px; right: 30px; height: 100%; position: absolute; overflow-wrap: break-word; }

::-webkit-scrollbar { display: none; }

::-webkit-scrollbar-thumb { display: none; }

#m_bizbar { background-color: rgb(252, 247, 182); border-top: 1px solid rgb(215, 216, 137); border-bottom: 1px solid rgb(215, 216, 137); box-shadow: rgba(0, 0, 0, 0.3) 0px 4px 4px; min-height: 39px; position: absolute; white-space: nowrap; width: 100%; }

#m_bizbar .button { display: inline-block; height: 28px; padding: 6px 0px 0px 6px; position: absolute; top: 7px; width: 28px; }

#m_bizbar .dismiss { right: 2px; }

#m_bizbar .refresh { right: 35px; }

#m_bizbarText { color: rgb(102, 102, 102); font-size: 12pt; margin: 12px 69px 15px 10px; position: relative; white-space: normal; }

.newToolbar { position: absolute; width: 100%; border: none; background-color: rgb(0, 32, 128); color: white; }

.newToolbarLandscape { height: 48px; }

.newToolbarPortrait { height: 40px; }

.appIconContainer { margin: 8px; float: left; }

.fileNameContainer { position: absolute; margin: 8px 0px; width: 100%; box-sizing: border-box; font-size: 14px; }

.newToolbarLandscape .fileNameContainer { padding: 0px 96px 0px 48px; line-height: 32px; }

.newToolbarPortrait .fileNameContainer { padding: 0px 80px 0px 40px; line-height: 24px; }

.newFontSizeBtnContainer { margin: 8px 8px 8px 0px; float: right; }

.newOicBtnContainer { margin: 8px 8px 8px 0px; float: right; }

.fileNameDiv { text-overflow: ellipsis; white-space: nowrap; overflow: hidden; }

#wildFireBannerContainer { width: 100%; background-color: rgb(243, 243, 243); z-index: 10; position: relative; height: 11%; border-bottom: 1px solid rgb(179, 179, 179); border-top: 1px solid rgb(77, 77, 77); display: table; top: 0px; max-height: 69px; }

#wildFireBannerContainer.wildFireIOS { font-family: -apple-system, "Helvetica Neue", "Lucida Grande"; }

#wildFireBannerContainer.wildFireAndroid { font-family: "Segoe UI"; }

.wildFireChildContainer { display: table-cell; vertical-align: middle; padding: 1.5% 0px 1.5% 0.5%; }

.wildFireBannerLink { color: rgb(43, 87, 154); padding: 0px 2.8% 0px 0px; text-decoration: none; text-align: center; }

.wildFireBannerLink.wildFireAndroid { font-size: 16px; }

.wildFireBannerLink.wildFireIOS { font-size: 16px; }

.wildFireBannerLink.wildFireSmallScreen { font-size: 13px; }

.wildFireAppIconContainer.wildFireIOS { background-color: rgb(47, 95, 163); border-radius: 10px; border: 7px solid rgb(47, 95, 163); padding: 0px; }

.wildFireAppIconContainer.wildFireAndroid { background-color: rgb(47, 95, 163); border: 5px solid rgb(47, 95, 163); padding: 0px; }

.wildFireDismissIconContainer { padding: 0px 0.75% 0px 2.8%; }

.wildFireTextContainer { padding: 0px 0px 0px 2%; color: rgb(0, 0, 0); }

.wildFireHeader.wildFireIOS { font-size: 21px; }

.wildFireHeader.wildFireIOS.wildFireSmallScreen { font-size: 17px; }

.wildFireHeader.wildFireAndroid { font-size: 17px; }

.wildFireHeader.wildFireAndroid.wildFireSmallScreen { font-size: 13px; }

.wildFireSubHeader { font-size: 14px; color: rgb(118, 118, 118); }

.wildFireSubHeader.wildFireIOS { font-size: 16px; }

.wildFireSubHeader.wildFireSmallScreen { font-size: 12px; }

@media print {
  .disablePrintAndDownload { display: none; }
}
